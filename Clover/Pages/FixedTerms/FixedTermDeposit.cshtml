@page
@model Clover.Pages.FIxedTermDepositModel
@{
    ViewData["Title"] = "Plazos Fijos";
}
<div class=" = fixedTermIntro">
<h1>@ViewData["Title"]</h1>

 <button><a href="~/FormNewFixedTerm" class="button">Nuevo plazo fijo</a></button>
 </div>


<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Usuario</th>
            <th>Cuenta</th>
            <th>Monto</th>
            <th>Fecha de Creacion</th>
            <th>Fecha de Finalizacion</th>
            <th>Tasa</th>
            <th>Estado</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var fixedTerm in Model.FixedTermDeposit)
        {
            <tr>
                <td>@fixedTerm.Id</td>
                <td>@fixedTerm.UserId</td>
                <td>@fixedTerm.AccountId</td>
                <td>@fixedTerm.Amount</td>
                <td>@fixedTerm.CreationDate</td>
                <td>@fixedTerm.ClosingDate</td>
                <td>@fixedTerm.NominalRate</td>
                <td>@fixedTerm.State</td>

            </tr>
        }
    </tbody>
</table>
